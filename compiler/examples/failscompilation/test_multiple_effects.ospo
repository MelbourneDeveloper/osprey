// Effect declarations 
effect IO {
    read: fn() -> string
    write: fn(string) -> Unit
}

effect Network {
    request: fn(string) -> string
}

// Function with multiple effects (as per spec)
fn fetchAndLog(url: string) -> string ![IO, Network] = {
    let result = perform Network.request(url)
    perform IO.write("Fetched: " + result)
    result
}

