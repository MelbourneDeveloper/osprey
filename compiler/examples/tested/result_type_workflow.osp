type NoError = NoError {}

fn main() -> int {
  print("=== Result Type Workflow Test ===\n")

  let s = "hello"
  let len_result = length(s)

  match len_result {
    Success { value } => {
      print("Length: ")
      print(toString(value))
      print("\n")
    }
    Error { message } => {
      print("Error getting length\n")
    }
  }
 
  0
} 