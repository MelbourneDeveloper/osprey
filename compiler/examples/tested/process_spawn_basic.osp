// Demonstrates basic spawnProcess function

print("=== Basic Process Spawning Test ===")

// Test 1: Simple echo command
let result = spawnProcess("echo 'Hello World'")
match result {
    Success { value } => print("Process result: ${value}")
    Error { message } => print("Process failed: ${message}")
}

print("=== Test Complete ===") 