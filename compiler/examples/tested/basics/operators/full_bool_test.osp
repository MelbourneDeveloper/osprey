fn isGreater(x, y) = x > y
fn isEqual(x, y) = x == y
fn getTrue() = true
fn getFalse() = false

let result1 = isGreater(x: 5, y: 3)
let result2 = isEqual(x: 10, y: 10)
let bool1 = getTrue()
let bool2 = getFalse()

print("=== Boolean Test Results ===")

print("5 > 3:")
match result1 {
    Success { value } => print(value)
    Error { message } => print("Error: ${message}")
}

print("10 == 10:")
match result2 {
    Success { value } => print(value)
    Error { message } => print("Error: ${message}")
}

print("true literal:")
print(bool1)
print("false literal:")
print(bool2) 