// String interpolation example
let name = "Alice"
let age = 25
let score = 95

print("Hello ${name}!")
print("You are ${age} years old")
print("Your score is ${score} points")
print("Next year you'll be ${age + 1}")

// Calculate something
let doubled = score * 2
print("Double your score: ${doubled}")

// Multiple expressions in one string
print("${name} (${age}) scored ${score}/${100}") 