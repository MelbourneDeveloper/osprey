{
  "name": "osprey-websocket-server",
  "version": "1.0.0",
  "description": "Osprey WebSocket Server with Playwright Testing",
  "scripts": {
    "test": "playwright test",
    "test:headed": "playwright test --headed",
    "test:debug": "playwright test --debug",
    "install-browsers": "playwright install",
    "test-osprey": "playwright test --grep osprey",
    "test-nodejs": "playwright test --grep nodejs",
    "test-all": "playwright test --grep 'osprey|nodejs'"
  },
  "dependencies": {
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0"
  },
  "keywords": ["osprey", "websocket", "server", "testing", "playwright"],
  "author": "Osprey Team",
  "license": "MIT"
}
